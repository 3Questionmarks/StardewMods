{
    "Changes": [

		//Properties///
        {
            "Action": "EditMap",
            "Target": "Maps/Custom_CapeStardew",
            "When": {
            "WarpNetworkTotems": true,
          },
            "PatchMode": "Overlay",
            "MapTiles": [
                {
                    "Position": { "X": 20, "Y": 94 },
                    "Layer": "Buildings",
                    "SetProperties": {
                        "WarpNetwork": "CapeIsland"
                    }
                }
            ]
        },
        {
            "LogName": "Add warp",
            "Action": "EditMap",
            "Target": "Maps/Custom_Cape",
            "When": {
            "WarpNetworkTotems": true,
          },
            "PatchMode": "Overlay",
            "MapTiles": [
                {
                    "Position": { "X": 4, "Y": 2 },
                    "Layer": "Buildings",
                    "SetProperties": {
                        "WarpNetwork": "cape"
                    }
                }
            ]
        },
        //Destinations//
        {
            "LogName": "Register location with warp network",
            "Action": "EditData",
            "Target": "Data/WarpNetwork/Destinations",
            "When": {
            "WarpNetworkTotems": true,
          },
            "Entries": {
                "cape": {
                    "location": "Custom_Cape",
                    "x": 4,
                    "y": 3,
                    "enabled": true,
                    "label": "{{i18n:LocationName.Cape}}",
                    "icon": "Cape"
                },
                "CapeIsland": {
                    "location": "Custom_CapeStardew",
                    "x": 21,
                    "y": 95,
                    "enabled": true,
                    "label": "{{i18n:LocationName.CapeIslandBottle}}",
                    "icon": "Cape"
                }
            }
        },
        
        //loadicons//
        {
            "Action": "Load",
            "Target": "Data/WarpNetwork/Icons/Cape",
            "When": {
            "WarpNetworkTotems": true,
          },
            "FromFile": "assets/CapeIcon.png"
        },
        
      //jaitems//
      
        {
            "LogName": "Register warp item",
            "Action": "EditData",
            "Target": "Data/WarpNetwork/WarpItems",
            "Entries": {
                "{{spacechase0.JsonAssets/ObjectId:Cape Warp Totem}}": {
                    "Destination": "cape",
                    "IgnoreDisabled": true,
                    "Consume": "true",
                    "Color": "#00aaff",
                }
            },
            "When": {
                "HasMod": "spacechase0.JsonAssets,DreamyGloom.Aronyredkku",
            "WarpNetworkTotems": true,
            }
        },
        
         {
            "LogName": "Register warp item2",
            "Action": "EditData",
            "Target": "Data/WarpNetwork/WarpItems",

            "Entries": {
                "{{spacechase0.JsonAssets/ObjectId:Sea Crystal Flask}}": {
                    "Destination": "CapeIsland",
                    "IgnoreDisabled": true,
                    "Consume": "false",
                    "Color": "#00aaff",
                }
            },
            "When": {
                "HasMod": "spacechase0.JsonAssets,DreamyGloom.Aronyredkku",
                "WarpNetworkTotems": true,
            }
        },
		
		
	
    ]
